package com.generation.f202205;

public class Main {

	public static void main(String[] args) {
		// Instancia de una clase
		//Objeto en UPPERCASE y variable en LOWERCASE
		Alumno alumno = new Alumno(); //Inicializamos la variable sin valores ni atributos
		Alumno alumno2 = new Alumno("Cristian", "Yañez" , 26 , "G1");//Inicializamos la variable con valores por default
		
		System.out.println(alumno.getNombre());
		System.out.println(alumno2.getNombre());
		
		//setter
		alumno.setNombre("Cris");
	}
}
